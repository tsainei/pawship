
<div class="container text-center mt-2">
<h1>These are all your matches!</h1>
<% @dogs.each do |dog| %>
  <div class="card-product text-start">
    <%= cl_image_tag(dog.photo.key, crop: :fit) %>
    <div class="card-product-infos ">
      <h2 class="mt-2">  <%= dog.name %>, <%= dog.age %></h2>
      <p class="text-left">Contact <%= dog.name %>'s owner <%= dog.user.email %></p>
      <%= link_to "Details", dog_path(dog.id), class: "btn mt-2 mb-2 btn-gray text-center" %>

    <% end %>
  </div>
</div>
 <%= link_to 'Find More Playdates', dogs_path, class: "btn m-3 btn-primary text-center" %>
</div>
